    <validationRules>
        <fullName>Restrict_Job_date_change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Job_Date__c ),
$User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Job_Date__c</errorDisplayField>
        <errorMessage>You cannot update the Job Date after the job is created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Use_only_active_projects</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Project__r.Status__c,&quot;I&quot;),$User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>The Project you have selected is inactive, please select an active project and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>job_completed_Validation</fullName>
        <active>true</active>
        <description>when job status is completed the user is not able to edit the record Apart from the user who has the permission to bypass the validation.</description>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE( Status__c ), &quot;Completed&quot;), $User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorMessage>You can&apos;t change the record information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_fields_validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK( Name  )  &amp;&amp;  ISBLANK( Job_Location__c )  &amp;&amp;  ISBLANK( Job_Date__c )</errorConditionFormula>
        <errorMessage>Please fill up the required fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_picklist_values_for_Admin</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Profile.Name =&apos;Hydro-X Admin&apos;,
TEXT( Status__c )!= &apos;Pending Billing Approval&apos;,
ISCHANGED( Status__c )

||
$Profile.Name =&apos;Hydro-X Admin&apos;,
TEXT( Status__c )!= &apos;Pending Supervisor Approval&apos;,
ISCHANGED( Status__c )
||
$Profile.Name =&apos;Hydro-X Admin&apos;,
TEXT( Status__c )!= &apos;Pending Admin Approval&apos;,
ISCHANGED( Status__c )
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Error: You cannot update the job to this status.</errorMessage>
