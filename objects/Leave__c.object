    <validationRules>
        <fullName>Status_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE( Status__c ), &quot;Approved&quot;), $User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorMessage>You cannot make changes to an approved Leave</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_leave_rejection</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    AND(
        TEXT( Status__c ) = &quot;Rejected&quot;, 
        ISBLANK( Reason_for_leave_rejection__c )), True, False
 )</errorConditionFormula>
        <errorDisplayField>Reason_for_leave_rejection__c</errorDisplayField>
        <errorMessage>Please mention a reason for leave rejection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Leave_Status_for_Operator</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name =&apos;Hydro-X Operator&apos;,
TEXT( Status__c )!=&apos;Requested&apos;,
ISCHANGED( Status__c )
 || 

 $Profile.Name =&apos;Hydro-X Operator&apos;,
TEXT( Status__c )!=&apos;Leave cancelled&apos;,
ISCHANGED( Status__c )
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Error: You cannot update the leave to this status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_date_should_not_less_than_start_date</fullName>
        <active>true</active>
        <errorConditionFormula>Leave_Start_date__c  &gt;  Leave_End_Date__c</errorConditionFormula>
        <errorDisplayField>Leave_Start_date__c</errorDisplayField>
        <errorMessage>Error -End Date should not be less than Start Date</errorMessage>
    </validationRules>
