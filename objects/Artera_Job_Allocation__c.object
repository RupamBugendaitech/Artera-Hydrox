        <fullName>Total_Billable_Hours__c</fullName>
        <externalId>false</externalId>
        <formula>if( ISNULL(Start_Time__c),0,if( Change_to_Non_Billable__c =True,0, IF(Job__r.Total_Job_Ticket_Hours__c &lt; Total_Hours__c , Job__r.Total_Job_Ticket_Hours__c, Total_Hours__c )))</formula>
