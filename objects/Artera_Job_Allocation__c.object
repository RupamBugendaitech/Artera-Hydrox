        <externalId>false</externalId>
        <formula>IF( ISNULL( Start_Time__c ),0,
IF(End_Time__c = Start_Time__c , 24,
IF(ISPICKVAL( Type__c , &apos;Leave&apos;),( End_Time__c - Start_Time__c ) /3600000 ,
IF(
MOD((( End_Time__c - Start_Time__c ) /3600000 - (Lunch_Time__c/60)),0.25) &lt; 0.125,
( End_Time__c - Start_Time__c ) /3600000 - (Lunch_Time__c/60) - MOD((( End_Time__c - Start_Time__c ) /3600000 - (Lunch_Time__c/60)),0.25),
( End_Time__c - Start_Time__c ) /3600000 - (Lunch_Time__c/60) +(0.25- MOD((( End_Time__c - Start_Time__c ) /3600000 - (Lunch_Time__c/60)),0.25))))))</formula>
