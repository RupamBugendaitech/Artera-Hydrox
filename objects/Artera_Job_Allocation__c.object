    <validationRules>
        <fullName>Job_status_completed_validation</fullName>
        <active>true</active>
        <description>when job status is completed the user is not able to edit the record Apart from the user who has the permission to bypass the validation.</description>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE( Status__c ) , &quot;Completed&quot;), $User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorMessage>You can&apos;t change the record information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Job_date_greater_than_prior_date</fullName>
        <active>false</active>
        <errorConditionFormula>Job_Assignment_Date_Field__c   &lt;   DATEVALUE(Job__r.CreatedDate)</errorConditionFormula>
        <errorDisplayField>Job_Assignment_Date_Field__c</errorDisplayField>
