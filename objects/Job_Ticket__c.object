    <validationRules>
        <fullName>Job_completed_validation</fullName>
        <active>true</active>
        <description>when job status is completed the user is not able to edit the record Apart from the user who has the permission to bypass the validation.</description>
        <errorConditionFormula>AND(ISPICKVAL(Job1__r.Status__c, &quot;Completed&quot;), $User.Bypass_Validation__c = FALSE)</errorConditionFormula>
