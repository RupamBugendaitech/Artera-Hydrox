    <validationRules>
        <fullName>Job_completed_validation</fullName>
        <active>true</active>
        <description>when job status is completed the user is not able to edit the record Apart from the user who has the permission to bypass the validation.</description>
        <errorConditionFormula>AND(ISPICKVAL( Job_Ticket__r.Job1__r.Status__c , &quot;Completed&quot;), $User.Bypass_Validation__c = FALSE)</errorConditionFormula>
        <errorMessage>You can&apos;t change the record information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Item_Validation_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND(IF(Unit_Item__r.Project__r.Project_Id__c  &lt;&gt; Job__r.Project_ID__c,True ,false),

 NOT( ISBLANK( Unit_Item__c ) )
)</errorConditionFormula>
        <errorMessage>You cannot save the record unless unit item is equals to project id.</errorMessage>
    </validationRules>
